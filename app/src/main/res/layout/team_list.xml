<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingVertical="3dp"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/rank_team_list"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1.5"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="100"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/score_team_list"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="100"
        android:textSize="12sp" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="10.5"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/profile1_team_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/teamMember1_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3.5"
                android:gravity="center_horizontal"
                android:text="팀원1"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/preferenceScore1_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="1"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/capability1_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:text="역량1"
                android:textSize="12sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/profile2_team_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/teamMember2_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3.5"
                android:gravity="center_horizontal"
                android:text="팀원2"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/preferenceScore2_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="2"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/capability2_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:text="역량2"
                android:textSize="12sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/profile3_team_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/teamMember3_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3.5"
                android:gravity="center_horizontal"
                android:text="팀원3"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/preferenceScore3_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="3"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/capability3_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:text="역량3"
                android:textSize="12sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/profile4_team_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/teamMember4_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3.5"
                android:gravity="center_horizontal"
                android:text="팀원4"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/preferenceScore4_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="4"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/capability4_team_list"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:text="역량4"
                android:textSize="12sp" />

        </LinearLayout>

    </LinearLayout>

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="＞"
        android:textSize="12sp" />

</LinearLayout>